<%= link_to 'sair', destroy_user_session_path, :method => :delete %>
<%= form_for current_user, as: 'post', url: url_for(action: :preview), html: {id: 'createBook'} do |f| %>

  <!-- W -->
  <div id="w">
    <%= image_tag 'w.jpg' %>
    <div class="userInput">
      <% @images.each do |i| %>
        <%= image_tag i %>
      <% end %>
    </div>
    <%= f.file_field :template_picture %>
    <%= f.hidden_field :template_picture_cache %>
    <div class="hit"></div>
  </div>

  <%= image_tag 'plus.png', id: 'plus' %>

  <!-- K -->
  <div id="k">
    <%= image_tag 'k.jpg' %>
    <div class="userInput">
      <% 5.times do |i| %>
        <%= image_tag "type_placeholder#{i+1}.jpg" %>
      <% end %>
    </div>
  </div>

  <%= f.submit type: 'image', src: asset_path('ok.png'), class: 'submit'%>

  <div id="textPanel">
    <%= f.label :template_text, "Conte a sua histÃ³ria" %><br>
    <%= f.text_area :template_text, placeholder: "Lorem ipsum dolor sit amet..." %>
  </div>

<% end %>
